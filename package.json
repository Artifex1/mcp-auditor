{
  "name": "mcp-server-example",
  "version": "1.0.0",
  "description": "Example MCP Server for Gemini CLI Extension",
  "type": "module",
  "main": "dist/mcp/server.js",
  "scripts": {
    "build": "tsc",
    "bundle": "tsup",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run bundle && npm run copy-binary",
    "build-parsers": "tsx scripts/build-parsers.ts",
    "copy-binary": "node scripts/copy-binary.js",
    "typecheck": "tsc --noEmit -p tsconfig.test.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@ast-grep/cli-darwin-arm64": "^0.40.0",
    "@ast-grep/cli-darwin-x64": "^0.40.0",
    "@ast-grep/cli-linux-arm64-gnu": "^0.40.0",
    "@ast-grep/cli-linux-x64-gnu": "^0.40.0",
    "@ast-grep/cli-win32-arm64-msvc": "^0.40.0",
    "@ast-grep/cli-win32-x64-msvc": "^0.40.0",
    "@types/node": "^20.11.25",
    "tree-sitter-cli": "^0.26.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "~5.4.5",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@toon-format/toon": "^2.0.0",
    "fast-glob": "^3.3.3",
    "web-tree-sitter": "^0.26.3",
    "zod": "^3.22.4"
  }
}